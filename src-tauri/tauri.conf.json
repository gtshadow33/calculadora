{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "calculadora",
  "version": "0.2.3",
  "identifier": "com.gts.calculadora",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "calculadora",
        "width": 300,
        "height": 480,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm"],
    "icon": [
      "icons/image.png"
    ]
  }
}
